<%= form_for zone do |f| %>
  <div>
    <%= f.label "titlexxx" %><br>
    <%= f.text_field :title, size: "30x12" %>
  </div>
  <div>
    <%= f.label "directordirector" %><br>
    <%= f.text_field :director, size: "30x12" %>
  </div>

  <div>
    <%= f.label "runtime_in_minutesruntime_in_minutes" %><br>
    <%= f.number_field :runtime_in_minutes, size: "30x12" %>
  </div>
  <div>
    <%= f.label "descriptiondescription" %><br>
    <%= f.text_area :description, size: "30x12" %>
  </div>
  <div>
    <%= f.label "poster_image_urlposter_image_url" %><br>
    <%= f.text_field :poster_image_url, size: "30x12" %>
  </div>
  <div>
    <%= f.label "release_daterelease_date" %><br>
    <%= f.date_field :release_date, size: "30x12" %>
  </div>
  <div>
    <%= f.submit "submit" %>
  </div>
<% end %> 

